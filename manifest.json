{
"name": "Cyberoam Autologin",
"version": "1.0",
"manifest_version":2,
"description": "It automates the process of Cyberoam Login, and I'm trying my best to implement a logout procedure when the browser exits.",
"options_page":"options.html",
"background":{"scripts":["jquery-1.8.3.min.js", "background.js"] },
"browser_action": {
    "default_icon":"icon_grey.png",
    "default_title":"Cyberoam"
    },
"permissions": ["http://*/*","https://*/*","tabs","notifications","contextMenus","webNavigation","webRequest","webRequestBlocking","storage"]
}